@{
    ViewData["Title"] = "Inicio";
    var ahora = DateTime.Now;
    var listaEjemplo = new List<string> { "Pan", "Leche", "Huevos", "Cereal" };
}
<h2>Bienvenido a Cibertec Demo</h2>
<p>Fecha y hora actual: <strong>@ahora</strong></p>
<h4>Ejemplo de listado con Razor:</h4>
<ul>
    @foreach (var item in listaEjemplo)
    {
        <li>@item</li>
    }
</ul>
<p>Total de elementos: <strong>@listaEjemplo.Count</strong></p>
